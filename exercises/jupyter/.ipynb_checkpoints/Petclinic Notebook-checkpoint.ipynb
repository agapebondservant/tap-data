   "metadata": {},
   "source": [
    "url = \"http://gemfire1-locator-0.gemfire1-locator.\" + os.environ[\"SESSION_NAMESPACE\"] + \":7070/geode/v1/clinicalDataModel\"\n",
    "\n",
    "def get_model():\n",
